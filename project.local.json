{
  "Change": {
    "Install": {
      "webBaseDirectory": "www",
      "webBaseURLPath": ""
    },
    "Storage": {
      "baseFTP": {
        "class": "\\Change\\Storage\\Engines\\LocalStorage",
        "basePath": "App/Storage/FTP"
      }
    },
    "Session": {
      "SaveHandler": {
        "type": "SimpleRedis",
        "options": {
          "server": {
            "host": "redis",
            "port": "6379",
            "timeout": 5
          },
          "ttl": 7200
        }
      }
    },
    "Mail": {
      "type": "smtp",
      "host": "smtp.gmail.com",
      "port": 465,
      "auth": true,
      "connection": "login",
      "username": "XXXX",
      "password": "XXXX",
      "ssl": "ssl",
      "fakemail": [
      ]
    },
    "Application": {
      "development-mode": true
    },
    "Presentation": {
      "AngularDevMode": true
    },
    "Dev": {
      "Resources": {
        "UI": true,
        "UI.common": true,
        "Theme": true,
        "Theme.base": true
      }
    },
    "Cache": {
      "block": false,
      "page": false,
      "Adapter": {
        "redis": {
          "server": {
            "host": "redis",
            "port": "6379"
          }
        }
      }
    },
    "Db": {
      "default": {
        "host": "mysql",
        "user": "root",
        "password": "root",
        "port": 3306,
        "longTransaction": 5
      }
    },
    "Http": {
      "Web": {
        "Monitoring": true
      },
      "Rest": {
        "Monitoring": true,
        "useCorrection": false
      },
      "Ajax": {
        "Monitoring": true
      }
    },
    "Logging": {
      "level": "DEBUG"
    }
  },
  "Rbs": {
    "Elasticsearch": {
      "indices": {
        "documents": {
          "host": "elasticsearch",
          "port": 9200
        }
      }
    }
  }
}
